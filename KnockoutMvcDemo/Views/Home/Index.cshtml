@{
  ViewBag.Title = "Knockout MVC";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
  <div class="span9">
    <div class="well" style="position: relative;">
      <a href="https://github.com/AndreyAkinshin/knockout-mvc">
        <img style="position: absolute; top: 0; right: 0; border: 0;" src="https://s3.amazonaws.com/github/ribbons/forkme_right_red_aa0000.png" alt="Fork me on GitHub">
      </a>

      <div style="text-align: center">
        <img src="@Url.Content("~/Images/Logo/logo180x100.png")" alt="Knockout MVC"/>
      </div>
      <h2 style="text-align: center; font-size: 20pt; margin-top: 10px; margin-bottom: 10px">Power of <a href="http://knockoutjs.com/">Knockout.js</a> for <a href="http://www.asp.net/mvc/mvc3">ASP.NET MVC</a>.</h2>
      <div style="margin-left: 50px; margin-right: 10px">
        @RenderPage(@Url.Content("~/Views/Home/ConceptTable2.cshtml"))
      </div>
    </div>
  </div>
  <div class="span3">
    <a href="http://www.perpetuumsoft.com/SharpShooter-Reports-Web.aspx" onClick="_gaq.push(['_trackEvent', 'Hyperlink', 'SharpShooterReportsWeb', 'Go to http://www.perpetuumsoft.com/SharpShooter-Reports-Web.aspx']);" target="_blank">
      <img src="@Url.Content("~/Images/SharpShooterReportsWeb.jpg")"/>
    </a>
    <a href="http://perfectwidgets.com/" onClick="_gaq.push(['_trackEvent', 'Hyperlink', 'PerfectWidgets', 'Go to http://perfectwidgets.com/']);" target="_blank">
      <img src="@Url.Content("~/Images/PerfectWidgets.jpg")" />
    </a>
  </div>
</div>
